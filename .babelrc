{
  "presets": [
    ["@babel/env", {
      "targets": {
        "chrome": "80",
        "firefox": "79",
        "edge": "84",
        "safari": "13.1"
      },
      "modules": "cjs"
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties"
  ],
  "ignore": ["**/node_modules/**"]
}
